{
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig to read more about this file */
    "lib": [
      "ES2023"
    ],
    "module": "NodeNext",
    "target": "ES2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "NodeNext",
    "paths": {
      "@routers/*": [
        "./src/routers/*",
        "./src/routers"
      ],
      "@models/*": [
        "./src/models/*",
        "./src/models"
      ],
      "@type/*": [
        "./src/types/*",
        "./src/types"
      ],
      "@/*": [
        "./src/*"
      ]
    },
    "typeRoots": [
      "./src/types",
      "./node_modules/@types"
    ]
  },
  "ts-node": {
    "require": [
      "tsconfig-paths/register"
    ]
  }
}